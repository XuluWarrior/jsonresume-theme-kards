<body id="top">

<!-- header
================================================== -->
<header>
    <div class="row">

        <div class="top-bar">
            <a class="menu-toggle" href="#"><span>Menu</span></a>

            <div class="logo">
                <a href="index.html">Resume</a>
            </div>

            <nav id="main-nav-wrap">
                <ul class="main-navigation">
                    <li class="current"><a class="smoothscroll"  href="#intro" title="">Home</a></li>
                    <li><a class="smoothscroll"  href="#about" title="">About</a></li>
                    <li><a class="smoothscroll"  href="#resume" title="">Resume</a></li>
                    <!--<li><a class="smoothscroll"  href="#portfolio" title="">Portfolio</a></li>-->
                    <!--<li><a class="smoothscroll"  href="#services" title="">Services</a></li>-->
                    <!--<li><a class="smoothscroll"  href="#contact" title="">Contact</a></li>-->
                </ul>
            </nav>
        </div> <!-- /top-bar -->

    </div> <!-- /row -->
</header> <!-- /header -->

{{#resume.basics}}
    <!-- intro section
  ================================================== -->
    <section id="intro">

        <div class="intro-overlay"></div>

        <div class="intro-content">
            <div class="row">

                <div class="col-twelve">

                    <h5>Hello, World.</h5>
                    <h1>I'm {{name}}.</h1>

                    <p class="intro-position">
                        <span>{{label}}</span>
                    </p>

                    <a class="button stroke smoothscroll" href="#about" title="">More About Me</a>

                </div>

            </div>
        </div> <!-- /intro-content -->

        <ul class="intro-social">
            {{#each profiles}}
                <li><a href="{{url}}"><i class="fa fa-{{toLowerCase network}}"></i></a></li>
            {{/each}}
        </ul> <!-- /intro-social -->

    </section> <!-- /intro -->
{{/resume.basics}}



<!-- about section
================================================== -->
<section id="about">

    {{#resume.basics}}
        <div class="row section-intro">
            <div class="col-twelve">

                <h5>About</h5>
                <h1>Let me introduce myself.</h1>

                <div class="intro-info">

                    {{#if image}}<img src="{{image}}" alt="Profile Picture">{{/if}}

                    <p class="lead">{{#markdown}}{{summary}}{{/markdown}}</p>
                </div>

            </div>
        </div> <!-- /section-intro -->
    {{/resume.basics}}

    <div class="row about-content">

        <div class="col-six tab-full">

            <h3>Profile</h3>
            <!--<p>Lorem ipsum Qui veniam ut consequat ex ullamco nulla in non ut esse in magna sint minim officia consectetur nisi commodo ea magna pariatur nisi cillum.</p>-->

            {{#resume.basics}}
                <ul class="info-list">
                    <li>
                        <strong>Fullname:</strong>
                        <span>{{name}}</span>
                    </li>
                    {{#if label}}
                        <li>
                            <strong>Job:</strong>
                            <span>{{label}}</span>
                        </li>
                    {{/if}}
                    {{#if url}}
                        <li>
                            <strong>Website:</strong>
                            <span>{{displayUrl url}}</span>
                        </li>
                    {{/if}}
                    {{#if email}}
                        <li>
                            <strong>Email:</strong>
                            <span>{{email}}</span>
                        </li>
                    {{/if}}

                </ul> <!-- /info-list -->
            {{/resume.basics}}
        </div>

        {{#if resume.skills}}
            <div class="col-six tab-full">

                <h3>Skills</h3>
                <!--<p>Lorem ipsum Qui veniam ut consequat ex ullamco nulla in non ut esse in magna sint minim officia consectetur nisi commodo ea magna pariatur nisi cillum.</p>-->

                <ul class="skill-bars">
                    {{#each resume.skills}}
                        <li>
                            <div class="progress percent{{skillLevel level}}"><span>{{skillLevel level}}%</span></div>
                            <strong>{{name}}</strong>
                        </li>
                    {{/each}}

                </ul> <!-- /skill-bars -->

            </div>
        {{/if}}
    </div>

    <div class="row button-section">
        <div class="col-twelve">
            <a href="#contact" title="Hire Me" class="button stroke smoothscroll">Hire Me</a>
            <a href="#" title="Download CV" class="button button-primary">Download CV</a>
        </div>
    </div>

</section> <!-- /process-->


<!-- resume Section
================================================== -->
<section id="resume" class="grey-section">

    <div class="row section-intro">
        <div class="col-twelve">

            <h5>Resume</h5>
            <h1>More of my credentials.</h1>

            <!--<p class="lead">Lorem ipsum Do commodo in proident enim in dolor cupidatat adipisicing dolore officia nisi aliqua incididunt Ut veniam lorem ipsum Consectetur ut in in eu do.</p>-->

        </div>
    </div> <!-- /section-intro-->

    {{#if resume.work}}
        <div class="row resume-timeline">


            <div class="col-twelve resume-header">

                <h2>Work Experience</h2>

            </div> <!-- /resume-header -->

            <div class="col-twelve">

                <div class="timeline-wrap">

                    {{#each resume.work}}
                        <div class="timeline-block">

                            <div class="timeline-ico">
                                <i class="fa fa-briefcase"></i>
                            </div>

                            <div class="timeline-header">
                                <h3>{{position}}</h3>
                                <p>{{date startDate}} - {{date endDate}}</p>
                            </div>

                            <div class="timeline-content">
                                <h4>{{name}}</h4>
                                <p>{{#markdown}}{{summary}}{{/markdown}}</p>
                            </div>

                        </div> <!-- /timeline-block -->
                    {{/each}}

                </div> <!-- /timeline-wrap -->

            </div> <!-- /col-twelve -->

        </div> <!-- /resume-timeline -->
    {{/if}}

    {{#if resume.volunteer}}
        <div class="row resume-timeline">


            <div class="col-twelve resume-header">

                <h2>Volunteer Work</h2>

            </div> <!-- /resume-header -->

            <div class="col-twelve">

                <div class="timeline-wrap">

                    {{#each resume.volunteer}}
                        <div class="timeline-block">

                            <div class="timeline-ico">
                                <i class="fa fa-handshake-o"></i>
                            </div>

                            <div class="timeline-header">
                                <h3>{{position}}</h3>
                                <p>{{date startDate}} - {{date endDate}}</p>
                            </div>

                            <div class="timeline-content">
                                <h4>{{organization}}</h4>
                                <p>{{#markdown}}{{summary}}{{/markdown}}</p>
                            </div>

                        </div> <!-- /timeline-block -->
                    {{/each}}

                </div> <!-- /timeline-wrap -->

            </div> <!-- /col-twelve -->

        </div> <!-- /resume-timeline -->
    {{/if}}

    {{#if resume.publications}}
        <div class="row resume-timeline">


            <div class="col-twelve resume-header">

                <h2>Publications</h2>

            </div> <!-- /resume-header -->

            <div class="col-twelve">

                <div class="timeline-wrap">

                    {{#each resume.publications}}
                        <div class="timeline-block">

                            <div class="timeline-ico">
                                <i class="fa fa-book"></i>
                            </div>

                            <div class="timeline-header">
                                <h3>{{name}}</h3>
                                <p>{{date releaseDate}}</p>
                            </div>

                            <div class="timeline-content">
                                <h4>{{publisher}}</h4>
                                <p>{{#markdown}}{{summary}}{{/markdown}}</p>
                            </div>

                        </div> <!-- /timeline-block -->
                    {{/each}}

                </div> <!-- /timeline-wrap -->

            </div> <!-- /col-twelve -->

        </div> <!-- /resume-timeline -->
    {{/if}}

    {{#if resume.education}}
        <div class="row resume-timeline">

            <div class="col-twelve resume-header">

                <h2>Education</h2>

            </div> <!-- /resume-header -->

            <div class="col-twelve">

                <div class="timeline-wrap">

                    {{#each resume.education}}
                        <div class="timeline-block">

                            <div class="timeline-ico">
                                <i class="fa fa-graduation-cap"></i>
                            </div>

                            <div class="timeline-header">
                                <h3>{{studyType}} Degree</h3>
                                <p>{{date startDate}} - {{date endDate}}</p>
                            </div>

                            <div class="timeline-content">
                                <h4>{{institution}}</h4>
                                <p>{{area}}</p>
                            </div>

                        </div> <!-- /timeline-block -->
                    {{/each}}

                </div> <!-- /timeline-wrap -->

            </div> <!-- /col-twelve -->

        </div> <!-- /resume-timeline -->
    {{/if}}


</section> <!-- /features -->


